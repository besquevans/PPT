<h1>Sign up</h1>

<%= form_for @user do |f| %>
  <%= f.label :account %>
  <%= f.text_field :account %>

  <%= f.label :email %>
  <%= f.text_field :email %>

  <%= f.label :password %>
  <%= f.text_field :password %>

  <%= f.label :password_confirmation %>
  <%= f.text_field :password_confirmation %>

  <%= f.submit "Sign up" %>
<% end %>